<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" stylesheets="@../../styles/main.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.belmanexam.ui.controllers.admin.AdminMenuComponent">
   <children>
      <Button fx:id="btnUsers" mnemonicParsing="false" onAction="#switchToUsers" prefHeight="45.0" prefWidth="150.0" styleClass="active-menu-button" text="Users">
         <VBox.margin>
            <Insets bottom="40.0" />
         </VBox.margin></Button>
      <Button fx:id="btnLogs" mnemonicParsing="false" onAction="#switchToLogs" prefHeight="45.0" prefWidth="150.0" styleClass="disabled-menu-button" text="Logs">
         <VBox.margin>
            <Insets />
         </VBox.margin></Button>
   </children>
   <padding>
      <Insets left="50.0" right="50.0" />
   </padding>
</VBox>

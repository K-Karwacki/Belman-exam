<AnchorPane prefHeight="600.0" prefWidth="1280.0" styleClass="bgc" stylesheets="@../../styles/main.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.belmanexam.ui.controllers.dashboards.DocumentationDashboardController">
    <children>
        <Label alignment="CENTER_RIGHT" layoutY="40.0" prefHeight="50.0" prefWidth="317.0" text="List of documentation">
            <padding>
                <Insets bottom="10.0" right="20.0" top="10.0" />
            </padding>
            <font>
                <Font name="Helvetica Neue" size="20.0" />
            </font>
         <styleClass>
            <String fx:value="header-title" />
            <String fx:value="left-side-header" />
         </styleClass>
        </Label>
        <HBox layoutX="946.0" layoutY="40.0" prefHeight="50.0" styleClass="radius" stylesheets="@../../styles/search-bar.css">
            <children>
                <TextField alignment="CENTER" prefHeight="55.0" prefWidth="240.0" promptText="Order number" stylesheets="@../../styles/search-bar.css">
                    <font>
                        <Font name="Helvetica Neue" size="18.0" />
                    </font>
                </TextField>
                <StackPane prefHeight="60.0" prefWidth="44.0">
                    <children>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../images/loop-icon.png" />
                            </image>
                        </ImageView>
                    </children>
                    <styleClass>
                        <String fx:value="green-button" />
                        <String fx:value="loop-icon-container" />
                    </styleClass>
                </StackPane>
            </children>
            <effect>
                <DropShadow height="5.0" radius="3.0" width="9.0" />
            </effect>
        </HBox>
      <HBox alignment="CENTER" layoutX="411.0" layoutY="45.0" spacing="33.0">
         <children>
            <Button mnemonicParsing="false" prefHeight="32.0" prefWidth="130.0" styleClass="green-button" text="Approved">
               <effect>
                  <DropShadow blurType="GAUSSIAN" offsetX="-3.0" offsetY="5.0">
                     <color>
                        <Color red="0.3400000035762787" green="0.3400000035762787" blue="0.3400000035762787" opacity="0.4572649598121643" />
                     </color>
                  </DropShadow>
               </effect>
            </Button>
            <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="130.0" style="-fx-border-color: #338D71; -fx-border-radius: 8; -fx-border-width: 2;" styleClass="gray-button" text="Pending" />
            <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="130.0" styleClass="gray-button" text="Rejected" />
         </children>
      </HBox>
      <TableView layoutX="64.0" layoutY="143.0" prefHeight="430.0" prefWidth="1169.0" styleClass="radius">
        <columns>
          <TableColumn prefWidth="216.0" text="Order number" />
          <TableColumn minWidth="0.0" prefWidth="135.0" text="Status" />
            <TableColumn minWidth="0.0" prefWidth="248.0" text="Documentation date" />
            <TableColumn minWidth="0.0" prefWidth="241.0" text="Documented by" />
        </columns>
      </TableView>
    </children>
</AnchorPane>

<VBox styleClass="bgc" stylesheets="@../../../styles/main.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.belmanexam.ui.controllers.admin.dashboards.LogsDashboardController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="49.0" styleClass="radius" stylesheets="@../../../styles/search-bar.css">
         <children>
            <Label alignment="CENTER_RIGHT" prefHeight="49.0" prefWidth="270.0" text="Logs">
               <padding>
                  <Insets bottom="10.0" right="20.0" top="10.0" />
               </padding>
               <font>
                  <Font size="23.0" />
               </font>
               <styleClass>
                  <String fx:value="left-side-header" />
                  <String fx:value="header-title" />
               </styleClass>
               <HBox.margin>
                  <Insets right="460.0" />
               </HBox.margin>
            </Label>
            <HBox prefHeight="49.0">
               <children>
                  <TextField alignment="CENTER" prefHeight="49.0" prefWidth="249.0" promptText="Search log" stylesheets="@../../../styles/search-bar.css">
                     <font>
                        <Font name="Arial" size="18.0" />
                     </font>
                  </TextField>
                  <StackPane prefHeight="49.0" prefWidth="49.0">
                     <children>
                        <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../images/loop-icon.png" />
                           </image>
                        </ImageView>
                     </children>
                     <styleClass>
                        <String fx:value="green-button" />
                        <String fx:value="loop-icon-container" />
                     </styleClass>
                  </StackPane>
               </children>
            </HBox>
         </children>
      </HBox>
      <TableView prefHeight="450.0" prefWidth="980.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
         <VBox.margin>
            <Insets left="65.0" top="50.0" />
         </VBox.margin>
      </TableView>
   </children>
   <padding>
      <Insets top="30.0" />
   </padding>
</VBox>
